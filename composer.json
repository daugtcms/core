{
  "name": "felixbeer/sitebrew",
  "description": "",
  "keywords": [
    "felixbeer",
    "sitebrew"
  ],
  "homepage": "https://github.com/felixbeer/sitebrew",
  "license": "MIT",
  "type": "library",
  "authors": [
    {
      "name": "Felix Beer",
      "email": "mail@felix.beer",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^8.1",
    "illuminate/support": "^10.10",
    "laravel/horizon": "^5.21",
    "league/flysystem-aws-s3-v3": "^3.0",
    "livewire/livewire": "^3.0",
    "plank/laravel-mediable": "^5.9",
    "spatie/laravel-data": "^3.9",
    "spatie/laravel-google-fonts": "^1.2",
    "spatie/laravel-sluggable": "^3.5",
    "spatie/laravel-translatable": "^6.5",
    "wire-elements/modal": "^2.0"
  },
  "require-dev": {
    "laravel/pint": "^1.13"
  },
  "autoload": {
    "psr-4": {
      "Sitebrew\\": "src"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Sitebrew\\Tests\\": "tests"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit",
    "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "Sitebrew\\SitebrewServiceProvider",
        "Sitebrew\\SitebrewBladeServiceProvider"
      ],
      "aliases": {
        "Sitebrew": "Sitebrew\\SitebrewFacade"
      }
    }
  }
}