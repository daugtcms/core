{
  "name": "daugtcms/core",
  "description": "",
  "keywords": [
    "felixbeer",
    "daugt"
  ],
  "homepage": "https://github.com/daugtcms/core",
  "license": "MIT",
  "type": "library",
  "authors": [
    {
      "name": "Felix Beer",
      "email": "mail@felix.beer",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^8.1",
    "lab404/laravel-impersonate": "^1.7",
    "laravel/framework": "^10.29",
    "laravel/horizon": "^5.21",
    "league/flysystem-aws-s3-v3": "^3.0",
    "livewire/livewire": "^3.0",
    "plank/laravel-mediable": "^5.9",
    "spatie/eloquent-sortable": "^4.0",
    "spatie/laravel-data": "^4.1",
    "spatie/laravel-google-fonts": "^1.2",
    "spatie/laravel-honeypot": "^4.4",
    "spatie/laravel-permission": "^6.1",
    "spatie/laravel-sluggable": "^3.5",
    "spatie/laravel-stripe-webhooks": "^3.6",
    "stripe/stripe-php": "^13.4",
    "symfony/http-client": "^6.3",
    "symfony/postmark-mailer": "^6.3",
    "ueberdosis/tiptap-php": "^1.3",
    "wire-elements/modal": "^2.0"
  },
  "require-dev": {
    "barryvdh/laravel-ide-helper": "^2.13",
    "laravel/pint": "^1.13"
  },
  "autoload": {
    "psr-4": {
      "Daugt\\": "src"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Daugt\\Tests\\": "tests"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit",
    "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "Daugt\\DaugtServiceProvider",
        "Daugt\\DaugtBladeServiceProvider",
        "Daugt\\DaugtMediableServiceProvider"
      ],
      "aliases": {
        "Daugt": "Daugt\\DaugtFacade"
      }
    }
  }
}
