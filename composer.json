{
  "name": "felixbeer/sitebrew",
  "description": "",
  "keywords": [
    "felixbeer",
    "sitebrew"
  ],
  "homepage": "https://github.com/felixbeer/sitebrew",
  "license": "MIT",
  "type": "library",
  "authors": [
    {
      "name": "Felix Beer",
      "email": "mail@felix.beer",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^8.1",
    "laravel/framework": "^10.29",
    "laravel/horizon": "^5.21",
    "league/flysystem-aws-s3-v3": "^3.0",
    "livewire/livewire": "^3.0",
    "plank/laravel-mediable": "^5.9",
    "spatie/eloquent-sortable": "^4.0",
    "spatie/laravel-data": "^3.9",
    "spatie/laravel-google-fonts": "^1.2",
    "spatie/laravel-permission": "^6.1",
    "spatie/laravel-sluggable": "^3.5",
    "spatie/laravel-stripe-webhooks": "^3.6",
    "stripe/stripe-php": "^13.4",
    "symfony/http-client": "^6.3",
    "symfony/postmark-mailer": "^6.3",
    "ueberdosis/tiptap-php": "^1.3",
    "wire-elements/pro": "^4.0"
  },
  "require-dev": {
    "barryvdh/laravel-ide-helper": "^2.13",
    "laravel/pint": "^1.13"
  },
  "autoload": {
    "psr-4": {
      "Sitebrew\\": "src"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Sitebrew\\Tests\\": "tests"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit",
    "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "Sitebrew\\SitebrewServiceProvider",
        "Sitebrew\\SitebrewBladeServiceProvider",
        "Sitebrew\\SitebrewMediableServiceProvider"
      ],
      "aliases": {
        "Sitebrew": "Sitebrew\\SitebrewFacade"
      }
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://wire-elements-pro.composer.sh"
    }
  ]
}
